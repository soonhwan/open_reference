{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components", {
        "ssr": true,
        "displayName": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "hooks": "./hooks",
          "styles": "./styles",
          "utils": "./utils"
        },
        "extensions": [".ts", ".tsx"]
      }
    ]
  ]
}
