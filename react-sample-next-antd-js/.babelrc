{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components", {
        "ssr": true,
        "displayName": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "hooks": "./hooks",
          "store": "./store",
          "reducers": "./reducers",
          "utils": "./utils",
          "styles": "./styles"
        },
        "extensions": [".js", ".jsx"]
      }
    ]
  ]
}