{
  "name": "OnestoreBooks-Front-end-2020",
  "version": "0.0.1",
  "description": "React.js ver. Build & Deploy Env.",
  "private": true,
  "homepage": "/resources/nbook20",
  "scripts": {
    "proxy": "http://localhost:8080",
    "build": "node commandNpm/build.js",
    "build:server": "node commandNpm/build.server.js",
    "build:all": "yarn build && yarn build:server",
    "test": "node commandNpm/test.js",
    "start": "node commandNpm/start.js",
    "start:json": "node commandNpm/start.json.js",
    "start:local": "node commandNpm/start.local.js",
    "start:dev": "node commandNpm/start.dev.js",
    "start:qa": "node commandNpm/start.qa.js",
    "start:ssr": "node _build_ssr/server",
    "start:ssrf": "nodemon --watch src/lib _build_ssr/server",
    "start-storybook": "start-storybook -p 3300",
    "build-storybook": "build-storybook",
    "storybook": "npm-run-all --parallel build-storybook start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "react-loadable/babel"
    ]
  },
  "dependencies": {
    "axios": "0.19.1",
    "axios-extensions": "3.0.6",
    "classnames": "2.2.6",
    "express": "4.17.1",
    "http-proxy-middleware": "1.0.4",
    "immer": "5.3.0",
    "prop-types": "^15.7.2",
    "query-string": "6.9.0",
    "react": "16.12.0",
    "react-app-polyfill": "1.0.5",
    "react-dev-utils": "10.1.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-loadable": "5.5.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "react-transition-group": "^4.4.1",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-api-middleware": "3.1.0",
    "redux-pender": "2.0.12",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "resolve": "1.14.2",
    "swiper": "^6.1.1",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "5.3.19",
    "@svgr/webpack": "5.0.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.5",
    "babel-preset-react-app": "9.1.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "3.4.2",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-react-app": "5.1.0",
    "eslint-config-standard": "14.1.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.5.3",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "5.0.2",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-style-loader": "5.1.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "sass-loader": "7.1.0",
    "storybook-react-router": "^1.0.8",
    "terser-webpack-plugin": "3.0.6",
    "url-loader": "1.1.2",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-node-externals": "1.7.2",
    "workbox-webpack-plugin": "4.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
